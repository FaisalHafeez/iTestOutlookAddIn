    <add name="hunterCVEntities" connectionString="metadata=res://*/huntercv.csdl|res://*/huntercv.ssdl|res://*/huntercv.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=invidia2.cq4vyxk9ulpx.eu-west-1.rds.amazonaws.com,1433;initial catalog=invidiadb;persist security info=True;user id=sa;password=031943889;multipleactiveresultsets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
